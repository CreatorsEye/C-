                BackColor = Color.Transparent;
                LinkColor = Color.FromArgb(181, 41, 42);
                ActiveLinkColor = Color.FromArgb(153, 34, 34);
                VisitedLinkColor = Color.FromArgb(181, 41, 42);
                LinkBehavior = LinkBehavior.NeverUnderline;
            }
